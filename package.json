    "postcss-styled-syntax": "^0.4.0",
    "process": "^0.11.10",
    "react-test-renderer": "^18.2.0",
    "sharp": "^0.31.1",
    "stylelint": "^15.6.2",
    "stylelint-config-standard": "^33.0.0"
  },
  "dependencies": {
    "@compiled/react": "^0.13.1",
    "@upstash/ratelimit": "^0.4.2",
    "@vercel/edge": "^0.3.4",
